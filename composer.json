{
    "name": "rbac-suite/omni-access",
    "description": "Laravel Advance Permission Manager (FREE) - RBAC with basic roles & permissions.",
    "type": "library",
    "version": "1.3.0",
    "keywords": [
        "laravel",
        "permissions",
        "rbac",
        "roles",
        "access-control"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "hinditutorpoint",
            "email": "hinditutorpoint@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "RbacSuite\\OmniAccess\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RbacSuite\\OmniAccess\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/hinditutorpoint/omni-access"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "RbacSuite\\OmniAccess\\OmniAccessServiceProvider"
            ],
            "aliases": {
                "OmniAccess": "RbacSuite\\OmniAccess\\Facades\\OmniAccess"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}